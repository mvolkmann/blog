---
layout: top-layout
---

<script src="/blog/js/topics.js"></script>

<nav>
  {{ renderNavList(collections.nav) | safe }}
</nav>

<section class="topic-background">
  <section class="topic-content">
    <button
      id="print-btn"
      onclick="print()" style="position: absolute; right: 0"
    >
      Print
    </button>
    <iframe
      id="topic-content"
      name="frame"
      onLoad="iframeLoaded(this)"
      src="../welcome/"
      title="topic content"></iframe>
  </section>
</section>